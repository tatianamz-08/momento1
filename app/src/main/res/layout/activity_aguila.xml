<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9A86C"
    tools:context=".Aguila">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aclonica"
        android:text="Aguila"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="28dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="363dp"
        android:layout_height="408dp"
        app:srcCompat="@drawable/aguila2"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="386dp"
        android:layout_height="163dp"
        android:fontFamily="@font/aclonica"
        android:text="Grandes aves de presa con una vista excepcional, poseen grandes picos y garras afiladas; habiles para cazar a gran velocidad."
        android:textAlignment="center"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="532dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#732002"
        android:text="Volver"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="731dp" />
</androidx.constraintlayout.widget.ConstraintLayout>